@model IEnumerable<WebApplication2.Models.BookList>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BookListName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BookListType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BookListContent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GenreType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BookListName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BookListType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BookListContent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GenreType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BookListID }) |
            @Html.ActionLink("Details", "Details", new { id=item.BookListID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.BookListID })
        </td>
    </tr>
}

</table>
